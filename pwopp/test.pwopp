def add(x):{
    if (x > 0) then 
        return x + add( x - 1)
    else 
        return x
};
print add(5);
def fib(n):{
    if n = 1 then return 1
    else if n = 0  then return 0
    else return fib(n - 1) + fib(n - 2)
};
print fib(5);

